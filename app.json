{
  "expo": {
    "name": "Snap & Send App",
    "description": "Receipt recognition and submission application live environment",
    "slug": "snap-and-send-app",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
